<h1><%= @hiker.name || @hiker.email %>'s Adventures</h1>

<% if @hiker.adventures != nil %>
  <div class="index">
    <ul>
      <% @adventures.each do |adventure| %>
      <h1><%= link_to adventure.title, adventure_path(adventure), class: 'adv-link', data: {id: adventure.id} %></h1>
      <% end %>
    </ul>
  </div>
<% end %>
