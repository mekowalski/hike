    </div>
  </div>

  <div class="col-sm-8 text-left">
    <h1>New Adventure</h1>

    <%= form_for(@adventure) do |f| %>
      <p>
        <%= f.label :title %>
      </p>
      <p>
        <%= f.text_field :title %>
      </p>

      <p>Add Treks</p>
      <p>Trek 1:</p>
      <p>Name <input type="text" /></p>
      <p>Description <textarea></textarea></p>
      <p>Trek 2:</p>
      <p>Name <input type="text" /></p>
      <p>Description <textarea></textarea></p>
      <p>
        <%= f.submit %>
      </p>
    <% end %>
  </div>
</div>

